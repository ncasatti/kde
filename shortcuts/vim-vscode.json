{
    "vim.leader": "<space>",
    "vim.visualModeKeyBindingsNonRecursive": [
        {"before": ["u"], "after": ["k"]},
        {"before": ["e"], "after": ["j"]},
        {"before": ["i"], "after": ["l"]},
        {"before": ["n"], "after": ["h"]},
        {"before": ["N"], "after": ["b"]},
        {"before": ["I"], "after": ["e"]},
        {"before": ["E"], "after": ["<C-d>"]},
        {"before": ["U"], "after": ["<C-u>"]},
        {"before": ["-"], "after": ["%"]},
        {"before": ["z"], "after": ["u"]},
        {"before": [";"], "after": ["y"]},
        {"before": ["Y"], "after": ["yy"]},
        {"before": ["P"], "after": ["\"", "0", "p"]},
        {"before": ["k"], "after": ["n"]},
        {"before": ["K"], "after": ["N"]},
        {"before": ["<leader>", "j"], "commands": [{"command": "editor.action.formatSelection"}]},
        {"before": ["<leader>", "P"], "commands": [{"command": "editor.action.showDefinitionPreviewHover"}]},
        {"before": ["<leader>", "p"], "commands": [{"command": "editor.action.triggerSuggest"}]}
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {"before": ["u"], "after": ["k"]},
        {"before": ["e"], "after": ["j"]},
        {"before": ["i"], "after": ["l"]},
        {"before": ["n"], "after": ["h"]},
        {"before": ["N"], "after": ["b"]},
        {"before": ["I"], "after": ["e"]},
        {"before": ["E"], "after": ["<C-d>"]},
        {"before": ["U"], "after": ["<C-u>"]},
        {"before": ["-"], "after": ["%"]},
        {"before": ["z"], "after": ["u"]},
        {"before": [";"], "after": ["y"]},
        {"before": ["Y"], "after": ["yy"]},
        {"before": ["P"], "after": ["\"", "0", "p"]},
        {"before": ["k"], "after": ["n"]},
        {"before": ["K"], "after": ["N"]},
        {"before": ["<C-i>"], "commands": [{"command": "workbench.action.focusRightGroup"}]},
        {"before": ["<C-n>"], "commands": [{"command": "workbench.action.focusFirstEditorGroup"}]},
        {"before": ["<C-i>"], "commands": [{"command": "workbench.action.focusRightGroup"}]},
        {"before": ["<C-n>"], "commands": [{"command": "workbench.action.focusLeftGroup"}]},
        {"before": [","], "after": ["i"]},
        {"before": ["<leader>", ","], "after": ["<S-i>"]},
        {"before": ["<leader>", "k"], "after": ["ggvGy"]},
        {"before": ["<leader>", "v"], "commands": [{"command": ":vsplit"}]},
        {"before": ["<leader>", "h"], "commands": [{"command": "workbench.action.splitEditorDown"}]},
        {"before": ["<leader>", "P"], "commands": [{"command": "editor.action.showDefinitionPreviewHover"}]},
        {"before": ["<leader>", "p"], "commands": [{"command": "editor.action.triggerSuggest"}]},
        {"before": ["<leader>", "g"], "commands": [{"command": "editor.action.peekTypeDefinition"}]},
        {"before": ["<leader>", "G"], "commands": [{"command": "editor.action.revealDefinitionAside"}]},
        {"before": ["<leader>", "d"], "commands": [{"command": "workbench.debug.action.focusRepl"}]},
        {"before": ["<leader>", "r"], "commands": [{"command": "workbench.view.debug"}]},
        {"before": ["<leader>", "a"], "commands": [{"command": "outline.focus"}]},
        {"before": ["<leader>", "j"], "commands": [{"command": "editor.action.formatSelection"}]},
        {"before": ["<leader>", "o"], "after": ["o", "esc"]},
        {"before": ["<leader>", "t"], "commands": [{"command": "terminal.focus"}]},
        {"before": ["<leader>", "."], "commands": [{"command": "editor.action.quickFix"}]},
        {"before": ["<leader>", "e"], "commands": [{"command": "bookmarks.jumpToNext"}]},
        {"before": ["<leader>", "u"], "commands": [{"command": "bookmarks.jumpToPrevious"}]},
        {"before": ["<leader>", "b"], "commands": [{"command": "bookmarks.toggle"}]}
    ],
    "vim.insertModeKeyBindingsNonRecursive": [
        {"before": ["<C-p>"], "commands": [{"command": "editor.action.triggerSuggest"}]}
    ]
}